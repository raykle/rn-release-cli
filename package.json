{
  "name": "rn-release-cli",
  "version": "1.1.1",
  "description": "Generic CLI tool for react-native project to automate executing cmd `react-native bundle`, `code-push release` and `bugsnag upload`.",
  "main": "./bin/rn-release-cli.js",
  "preferGlobal": true,
  "bin": {
    "rn-release": "./bin/rn-release-cli.js"
  },
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raykle/rn-release-cli.git"
  },
  "homepage": "https://github.com/raykle/rn-release-cli",
  "scripts": {
    "start": "node ./bin/rn-release-cli.js",
    "start:debug": "node --inspect-brk ./bin/rn-release-cli.js",
    "release": "release-it",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": {
    "name": "raykle",
    "email": "raykle.up@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "react-native",
    "code-push",
    "bugsnag",
    "release",
    "cli"
  ],
  "dependencies": {
    "bugsnag-sourcemaps": "^1.0.4",
    "chalk": "^3.0.0",
    "commander": "^5.0.0",
    "cross-spawn": "^7.0.1",
    "empty-dir": "^2.0.0",
    "inquirer": "^7.1.0",
    "mkdirp": "^1.0.3",
    "ora": "^4.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "release-it": "^12.6.3"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
